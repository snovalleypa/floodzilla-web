@{ 
    Layout = "_AdminLayout";
    ViewData["Title"] = "Role Tests";
}

<div class="row">
    <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">
        <div class="card card-info">
            <div class="card-header">
                <div class="card-title">Role Tests</div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col">
        Roles:
        <ul>
@{
        foreach (var role in ViewBag.Roles)
        {
            <li>@role.Name</li>
        }
}
        </ul><br />
        @ViewBag.ExtraText<br />
    </div>
</div>

<partial name="_showMessage" />

@section Scripts {
<script>
</script>
}
